{
  "name": "@iexec/subgraph-goerli",
  "version": "5.0.0",
  "scripts": {
    "build":           "graph codegen && graph build",
    "auth":            "graph auth https://api.thegraph.com/deploy/",
    "create-local":    "graph create iexecblockchaincomputing/iexec-poco-v5-goerli --node http://localhost:8020",
    "create-thegraph": "graph create iexecblockchaincomputing/iexec-poco-v5-goerli --node https://api.thegraph.com/deploy/",
    "create-iexec":    "graph create iexecblockchaincomputing/iexec-poco-v5-goerli --node http://thegraph.iex.ec:8020",
    "deploy-local":    "graph deploy iexecblockchaincomputing/iexec-poco-v5-goerli --node http://localhost:8020            --ipfs http://localhost:5001",
    "deploy-thegraph": "graph deploy iexecblockchaincomputing/iexec-poco-v5-goerli --node https://api.thegraph.com/deploy/ --ipfs https://api.thegraph.com/ipfs/",
    "deploy-iexec":    "graph deploy iexecblockchaincomputing/iexec-poco-v5-goerli --node http://thegraph.iex.ec:8020      --ipfs http://thegraph.iex.ec:5001"
  },
  "devDependencies": {
    "@graphprotocol/graph-cli": "^0.18.0",
    "@graphprotocol/graph-ts": "^0.18.1"
  },
  "dependencies": {
    "@iexec/poco": "5.0.0",
    "@iexec/solidity": "0.1.0-beta.13",
    "babel-polyfill": "^6.26.0",
    "babel-register": "^6.26.0"
  }
}
